[user]
	email = connorc0627@gmail.com
	name = Connor Chang
[alias]
	gr1 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''		   %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
	gr2 = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)'
	gra = log --graph --all --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''		   %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
	co = checkout
	s = status
	ss = status --untracked-files=no
	fpush = push --force-with-lease
	commit-fix = commit --amend --no-edit
[init]
	defaultBranch = main
[safe]
	directory = /opt/flutter
[pull]
	ff = only
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[push]
	autoSetupRemote = true
